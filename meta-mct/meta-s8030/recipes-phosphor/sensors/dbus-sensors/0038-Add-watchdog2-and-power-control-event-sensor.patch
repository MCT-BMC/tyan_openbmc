         {"BMC_Reboot","mgtsubsyshealth"},    
         {"PSU_STATUS","power_supply"}, 
-        {"IPMI_POWER_CYCLE","fru_state"}    
+        {"IPMI_POWER_CYCLE","fru_state"},
+        {"IPMI_POWER_ON","fru_state"},
+        {"IPMI_POWER_OFF","fru_state"},
+        {"IPMI_POWER_SOFT","fru_state"},
+        {"IPMI_POWER_RESET","fru_state"},
+        {"WATCHDOG2","watchdog2"}
     };
 
 struct eventSensor 
