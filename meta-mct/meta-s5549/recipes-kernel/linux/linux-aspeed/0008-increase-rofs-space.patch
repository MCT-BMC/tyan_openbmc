@@ -21,12 +21,12 @@ partitions {
 	};
 
 	rofs@c0000 {
-		reg = <0x4c0000 0x1740000>;
+		reg = <0x4c0000 0x2340000>;
 		label = "rofs";
 	};
 
 	rwfs@1c00000 {
-		reg = <0x1c00000 0x400000>;
+		reg = <0x2800000 0x1800000>;
 		label = "rwfs";
